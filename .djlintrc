{
    "profile": "django",
    "indent": 2,
    "max_line_length": 120,
    "format_css": true,
    "format_js": true,
    "ignore": "H006,H013,H030,H031",
    "extension": "html",
    "exclude": "venv/,node_modules/,.git/,__pycache__/,*.min.*",
    "quiet": false,
    "check": true,
    "format": true,
    "preserve_leading_space": true,
    "preserve_blank_lines": true,
    "close_void_tags": true,
    "format_attribute_template_tags": true,
    "max_blank_lines": 2,
    "blank_line_after_tag": "load,extends,include",
    "blank_line_before_tag": "block,if,for,with",
    "custom_blocks": "component,cache,compress,verbatim",
    "custom_html": "htmx,alpine",
    "use_gitignore": true,
    "warn": false
}